{
  "name": "playground-jest-koa-mongodb",
  "version": "1.0.0",
  "description": "A playground for Jest with Koa and MongoDB.",
  "author": "Roberto Achar <robertoachar@gmail.com>",
  "scripts": {
    "lint": "jest --config jest-eslint.config.js",
    "lint:watch": "jest --config jest-eslint.config.js --watch",
    "jest": "jest --config jest-test.config.js",
    "jest:coverage": "jest --config jest-test.config.js --coverage",
    "jest:watch": "jest --config jest-test.config.js --watch",
    "start": "nodemon main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@koa/cors": "3.0.0",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "koa": "2.11.0",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "5.2.0",
    "koa-logger": "3.2.1",
    "koa-router": "7.4.0",
    "mongoose": "5.8.4",
    "uuid": "3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@shelf/jest-mongodb": "1.1.3",
    "@types/jest": "24.0.25",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "jest-runner-eslint": "0.7.5",
    "jest-watch-typeahead": "0.4.2",
    "mongodb-memory-server": "6.2.1",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "supertest": "4.0.2"
  },
  "license": "MIT"
}
